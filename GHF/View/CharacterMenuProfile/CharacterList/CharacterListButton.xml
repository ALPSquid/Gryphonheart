<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ 
    http://wowprogramming.com/FrameXML/UI.xsd">
  <CheckButton name="GHF_CharacterListButtonTemplate" hidden="false" virtual="true">
    <CheckedTexture inherits="UIPanelButtonUpTexture" />
    <Layers>
      <Layer>
        <FontString name="$parentNameLabel" text="" inherits="SystemFont_Med2" parentKey="NameLabel">
          <Anchors>
            <Anchor point="TOPLEFT"/>
            <Anchor point="TOPRIGHT">
              <Offset>
                <AbsDimension x="-15" y="0" />
              </Offset>
            </Anchor>
          </Anchors>
          <FontHeight>
            <AbsValue val="11"/>
          </FontHeight>
          <Color r="1" g="0.8196079" b="0"/>
          <Shadow>
            <Color r="0" g="0" b="0"/>
            <Offset>
              <AbsDimension x="1" y="-1"/>
            </Offset>
          </Shadow>
        </FontString>
      </Layer>
    </Layers>
  </CheckButton>
</Ui>
